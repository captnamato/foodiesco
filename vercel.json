{
  "version": 2,
  "name": "foodiesco",
  "buildCommand": "cd foodies-frontend && npm run build",
  "outputDirectory": "foodies-frontend/build",
  "installCommand": "cd foodies-frontend && npm install",
  "framework": "create-react-app",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
